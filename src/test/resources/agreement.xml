<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://netgrif.github.io/petriflow/petriflow.schema.xsd">
	<id>Agreement</id>
	<initials>Agr</initials>
	<title>Agreement</title>
	<icon>device_hub</icon>
	<defaultRole>true</defaultRole>
	<anonymousRole>true</anonymousRole>
	<transitionRole>false</transitionRole>
	<data type="text">
		<id>contract_name</id>
		<title>Contract name</title>
		<placeholder>Input contract name</placeholder>
	</data>
	<data type="text">
		<id>customer_details</id>
		<title/>
		<init>Customer details</init>
	</data>
	<data type="taskRef">
		<id>taskRef_0</id>
		<title/>
		<init>t1</init>
	</data>
	<data type="enumeration_map">
		<id>issue_type</id>
		<title>Select issue type</title>
		<options>
			<option key="formal">formal</option>
			<option key="fundamental">fundamental</option>
		</options>
	</data>
	<data type="taskRef">
		<id>taskRef_1</id>
		<title/>
		<init>t4</init>
	</data>
	<data type="taskRef">
		<id>taskRef_result</id>
		<title/>
		<init>t13</init>
	</data>
	<data type="text">
		<id>decision_result</id>
		<title>Agreement decision</title>
	</data>
	<transition>
		<id>t1</id>
		<x>60</x>
		<y>340</y>
		<label>contract</label>
		<layout>
			<offset>0</offset>
		</layout>
		<dataGroup>
			<id>t1_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>contract_name</id>
				<logic>
					<behavior>editable</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>0</y>
					<rows>1</rows>
					<cols>3</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>customer_details</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>2</x>
					<y>2</y>
					<rows>1</rows>
					<cols>2</cols>
					<template>material</template>
					<appearance>standard</appearance>
				</layout>
			</dataRef>
		</dataGroup>
	</transition>
	<transition>
		<id>t4</id>
		<x>60</x>
		<y>420</y>
		<label>comment</label>
		<dataGroup>
			<id>t4_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>issue_type</id>
				<logic>
					<behavior>editable</behavior>
					<behavior>required</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>2</y>
					<rows>1</rows>
					<cols>2</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
		</dataGroup>
	</transition>
	<transition>
		<id>t6</id>
		<x>140</x>
		<y>340</y>
		<label>summary A</label>
		<dataGroup>
			<id>t6_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>taskRef_1</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>1</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>taskRef_0</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>2</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
			<dataRef>
				<id>taskRef_result</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>0</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
		</dataGroup>
	</transition>
	<transition>
		<id>t13</id>
		<x>60</x>
		<y>500</y>
		<label>result</label>
		<dataGroup>
			<id>t13_0</id>
			<cols>4</cols>
			<layout>grid</layout>
			<dataRef>
				<id>decision_result</id>
				<logic>
					<behavior>visible</behavior>
				</logic>
				<layout>
					<x>0</x>
					<y>0</y>
					<rows>1</rows>
					<cols>4</cols>
					<template>material</template>
					<appearance>outline</appearance>
				</layout>
			</dataRef>
		</dataGroup>
	</transition>
</document>